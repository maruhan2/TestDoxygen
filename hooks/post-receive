#!/bin/sh

export GIT_WORK_TREE = /Users/maruhanpark/TestDox
export GIT_DIR = /Users/maruhanpark/TestDox/.git
unset GIT_INDEX_FILE

mkdir a

GEN_DIR= ../
mkdir a
